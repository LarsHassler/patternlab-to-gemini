{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {},
  "rules": {
    "block-scoped-var": 1,
    "camelcase": 0,
    "comma-spacing": [1, {"before": false, "after": true}],
    "consistent-return": 2,
    "curly": [2, "all"],
    "dot-notation": [1, { "allowKeywords": true }],
    "eqeqeq": [2, "allow-null"],
    "guard-for-in": 2,
    "indent": [
      1, 2, {
        "SwitchCase": 1,
        "VariableDeclarator": 2,
        "MemberExpression": 1
      }
    ],
    "lines-around-comment": [1, {
      "beforeBlockComment": true,
      "beforeLineComment": true,
      "allowBlockStart": true,
      "allowObjectStart": true,
      "allowArrayStart": true
    }],
    "key-spacing": 0,
    "keyword-spacing": 2,
    "new-cap": 0,
    "no-alert": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-bind": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-mixed-requires": 0,
    "no-multi-str": 2,
    "no-multi-spaces": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-obj-calls": 2,
    "no-param-reassign": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-sparse-arrays": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-with": 2,
    "quotes": [0, "single"],
    "radix": 2,
    "semi": [0, "never"],
    "strict": 0,
    "space-before-blocks": 2,
    "space-before-function-paren": 0,
    "space-in-parens": [1, "never"],
    "space-infix-ops": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "valid-jsdoc": 0,
    "vars-on-top": 0,
    "wrap-iife": [2, "inside"]
  }
}